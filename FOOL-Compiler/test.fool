let

  class List (f:int, r:List) {
    fun first:int() f;
    fun rest:List() r;
  }

  fun printList:List (l:List)
      let
        fun makeList:List (l:List, i:int) new List (i,l);
      in
         if (l == null)
           then {null}
           else {makeList(printList(l.rest()),print(l.first()))};

  var l:List = new List (2,
                    new List(1,
                        new List(4,
                            new List (3,
                                new List(2,
                                    new List(5,null))))));

in printList(l);

